{
  "name": "popular-standards-api",
  "version": "0.1.0",
  "description": "An API web service for popular standards such as Language or Country codes ",
  "main": "__dist/app/main.js",
  "scripts": {
    "build": "tsc",
    "development": "NODE_ENV=development nodemon -L --watch app --ext js,json,ts --exec 'ts-node ./app/main.ts'",
    "format": "prettier --config .prettierrc '**/*.ts' --write",
    "lint": "eslint . --ext .js,.ts",
    "start": "node .",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DavidSedman/popular-standards-api.git"
  },
  "keywords": [
    "ISO",
    "3166",
    "ISO",
    "4217",
    "ISO",
    "639",
    "Node",
    "TypeScript"
  ],
  "author": {
    "name": "David Sedman",
    "email": "sedman.david@gmail.com",
    "url": "https://github.com/DavidSedman/popular-standards-api/issues"
  },
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/DavidSedman/popular-standards-api/issues"
  },
  "homepage": "https://github.com/DavidSedman/popular-standards-api#readme",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "openapi-types": "^9.3.1",
    "swagger-ui-express": "^4.1.6"
  }
}
